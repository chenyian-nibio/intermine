<?xml version="1.0"?>
<classes>
	<class name="DrugProteinInteraction" is-interface="true">
		<reference name="protein" referenced-type="Protein" reverse-reference="drugs"/>
		<reference name="drug" referenced-type="Drug" reverse-reference="targetProteins"/>
		<collection name="publications" referenced-type="Publication"/>
		<collection name="dataSets" referenced-type="DataSet"/>
    </class>

	<class name="Protein" is-interface="true">
		<collection name="drugs" referenced-type="DrugProteinInteraction" reverse-reference="protein"/>
    </class>

	<class name="DrugType" is-interface="true">
		<attribute name="name" type="java.lang.String"/>
    </class>

	<class name="Drug" extends="Compound" is-interface="true">
		<attribute name="genericName" type="java.lang.String"/>
		<attribute name="drugBankId" type="java.lang.String"/>
		<attribute name="keggDrugId" type="java.lang.String"/>
		<attribute name="description" type="java.lang.String"/>
		<attribute name="casRegistryNumber" type="java.lang.String"/>
		<reference name="hetGroup" referenced-type="HetGroup" reverse-reference="drugs"/>
		<reference name="chebiCompound" referenced-type="ChebiCompound" reverse-reference="drugs"/>
		<reference name="pubChemCompound" referenced-type="PubChemCompound" reverse-reference="drugs"/>
		<reference name="protein" referenced-type="Protein" />
		<collection name="drugTypes" referenced-type="DrugType"/>		
		<collection name="brandNames" referenced-type="Synonym"/>
		<collection name="targetProteins" referenced-type="DrugProteinInteraction" reverse-reference="drug"/>
    </class>

    <class name="ChebiCompound" extends="Compound" is-interface="true">
		<attribute name="chebiId" type="java.lang.String"/>
		<collection name="drugs" referenced-type="Drug" reverse-reference="chebiCompound"/>
    </class>
    
    <class name="HetGroup" extends="Compound" is-interface="true">
		<attribute name="hetId" type="java.lang.String"/>
		<collection name="drugs" referenced-type="Drug" reverse-reference="hetGroup"/>
    </class>

	<class name="PubChemCompound" extends="Compound" is-interface="true">
		<attribute name="pubChemCid" type="java.lang.String"/>
		<collection name="drugs" referenced-type="Drug" reverse-reference="pubChemCompound"/>
    </class>

    <class name="Compound" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<reference name="compoundGroup" referenced-type="CompoundGroup" reverse-reference="compounds"/>
    </class>

    <class name="CompoundGroup" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<collection name="compounds" referenced-type="Compound" reverse-reference="compoundGroup"/>
    </class>

</classes>
