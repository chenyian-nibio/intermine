<?xml version="1.0"?>
<classes>
	<class name="StitchInteraction" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="evidence" type="java.lang.String"/>
		<attribute name="score" type="java.lang.Integer"/>
		<reference name="compound" referenced-type="PubChemCompound" reverse-reference="proteinInteractions"/>
		<collection name="proteins" referenced-type="Protein" reverse-reference="chemicalInteractions"/>
		<collection name="dataSets" referenced-type="DataSet"/>
    </class>

	<class name="Protein" is-interface="true">
		<collection name="chemicalInteractions" referenced-type="StitchInteraction" reverse-reference="proteins"/>
		<collection name="compoundGroups" referenced-type="CompoundGroupInteraction" reverse-reference="protein"/>
    </class>

    <class name="ChebiCompound" extends="Compound" is-interface="true">
		<attribute name="chebiId" type="java.lang.String"/>
		<collection name="pubChemCompounds" referenced-type="PubChemCompound" reverse-reference="chebiCompounds" />
    </class>

	<class name="PubChemCompound" extends="Compound" is-interface="true">
		<attribute name="pubChemCid" type="java.lang.String"/>
		<collection name="chebiCompounds" referenced-type="ChebiCompound" reverse-reference="pubChemCompounds"/>
		<collection name="proteinInteractions" referenced-type="StitchInteraction" reverse-reference="compound" />
    </class>

    <class name="Compound" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<reference name="compoundGroup" referenced-type="CompoundGroup" reverse-reference="compounds"/>
    </class>


	<!-- For post processing; need the source pubchem to integrate CompoundGroup info -->	
    <class name="CompoundGroup" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<attribute name="name" type="java.lang.String"/>
		<collection name="compounds" referenced-type="Compound" reverse-reference="compoundGroup"/>
		<collection name="proteins" referenced-type="CompoundGroupInteraction" reverse-reference="compoundGroup" />
    </class>

	<class name="CompoundGroupInteraction" is-interface="true">
		<attribute name="identifier" type="java.lang.String"/>
		<reference name="compoundGroup" referenced-type="CompoundGroup" reverse-reference="proteins"/>
		<reference name="protein" referenced-type="Protein" reverse-reference="compoundGroups"/>
		<reference name="dataSet" referenced-type="DataSet"/>
    </class>

</classes>
